{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "kvk_number": {
      "type": "string",
      "required": false,
      "configurable": true,
      "maxLength": 20
    },
    "company_name": {
      "type": "string",
      "required": false,
      "configurable": true
    },
    "street_address": {
      "type": "string",
      "required": false,
      "configurable": true
    },
    "postal_code": {
      "type": "string",
      "required": false,
      "configurable": true,
      "maxLength": 10
    },
    "city": {
      "type": "string",
      "required": false,
      "configurable": true
    },
    "country": {
      "type": "string",
      "required": false,
      "configurable": true,
      "default": "Netherlands"
    },
    "sector": {
      "type": "enumeration",
      "enum": [
        "horeca",
        "winkeliers(retail)",
        "Sport&Recreatie",
        "dienstverlenen (lokaal, b2c)",
        "zakelijk dienstverlener(B2B)",
        "eten bestellen / bezorgdiensten",
        "hulpverleners / maatschappelijk (stichtingen, vereningen)"
      ],
      "required": true,
      "configurable": true
    },
    "sub_sector": {
      "type": "enumeration",
      "enum": [
        "Aannemer",
        "APK Keuringsstation",
        "Airco installateur",
        "Accountant",
        "Alarm installateur",
        "Arbeidsrechtadvocaat",
        "Autogarage",
        "Architect",
        "Autopoetsbedrijf",
        "Autoschadeherstel",
        "Bandenservice",
        "Bedrijfsvideo & promotie",
        "Belastingadvies en aangifte",
        "Beveilingsdiensten",
        "Binnenhuisarchitect",
        "Carwash",
        "Catering Service",
        "Container reiniging",
        "CV installateur",
        "Chauffeur",
        "Computerservice",
        "Copywriter",
        "Dakdekkers",
        "Dierencrematorium",
        "Elektricien",
        "Evenementorganisatie",
        "DJ",
        "Edelsmid",
        "Familierecht advocaat",
        "Fietsenmaker",
        "Feest- en vergaderruimte verhuur",
        "Fotograaf",
        "Glazenwasser",
        "Grafisch ontwerper",
        "Gevelreiniger",
        "Glaszetter",
        "Graffiti verwijderen",
        "Hovenier",
        "Hondentrimsalon",
        "Hondenuitlaatservice",
        "Huiswerkbegeleiding",
        "Hypotheekadviseur",
        "Installateur zonnepanelen",
        "Klussenbedrijf",
        "Keukenmontage",
        "Kinderopvang",
        "KLusbedrijf",
        "Loodgieter"
      ],
      "required": false,
      "configurable": true
    },
    "plan": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::plan.plan",
      "inversedBy": "users"
    },
    "subscription_status": {
      "type": "enumeration",
      "enum": ["active", "inactive", "cancelled", "trial", "expired"],
      "default": "inactive"
    },
    "subscription_start_date": {
      "type": "date"
    },
    "subscription_end_date": {
      "type": "date"
    },
    "mollie_customer_id": {
      "type": "string",
      "unique": false
    },
    "billing_period": {
      "type": "enumeration",
      "enum": ["monthly", "yearly"],
      "default": "monthly"
    },
    "last_payment_date": {
      "type": "datetime"
    },
    "total_paid": {
      "type": "decimal",
      "default": 0
    },
    "credits_available": {
      "type": "decimal",
      "default": 0
    },
    "current_subscription_id": {
      "type": "string",
      "unique": false
    },
    "trial_start_date": {
      "type": "datetime"
    },
    "trial_end_date": {
      "type": "datetime"
    },
    "is_trial_active": {
      "type": "boolean",
      "default": true
    },
    "plan_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::plan.plan",
      "inversedBy": "users"
    }
  }
}